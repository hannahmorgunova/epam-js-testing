{
  "name": "js-atm-program",
  "version": "1.0.0",
  "description": "Automated Testing Mentoring with JS: Basic",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "api_test": "mocha tests/specs/api/*.spec.js --timeout 40000",
    "unit_test": "mocha tests/specs/numbersValidator/*.spec.js",
    "coverage": "c8 npm run unit_test",
    "lint": "eslint ./ --fix",
    "mocha_test": "wdio run src/config/mochaWdio.conf.js",
    "cucumber_test": "wdio run src/config/cucumberWdio.conf.js",
    "allure": "allure generate allure-results --clean && allure open"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:hannamarhunova/js-atm-program.git"
  },
